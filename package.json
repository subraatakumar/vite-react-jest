{
  "name": "vite-react-jest",
  "version": "1.0.3",
  "description": "A custom Vite template for React with Jest testing setup",
  "keywords": [
    "vite",
    "react app",
    "jest",
    "vite react jest template",
    "subrata das"
  ],
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "format": "prettier --write .",
    "prepare": "if [ \"$CI\" != \"true\" ]; then husky; fi"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.3",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  },
  "files": [
    "src",
    "vite.config.js",
    "index.html",
    "package.json",
    "eslint.config.js",
    "setupTests.js"
  ],
  "publishConfig": {
    "access": "public"
  }
}
